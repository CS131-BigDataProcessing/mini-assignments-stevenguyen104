process {
	executor = 'local'
	cpus = 2
	memory = '4 GB'
	time = '1h'
	errorStrategy = 'retry'
	maxRetries = 2
}

workDir = './work'
report {
	enabled = true
	overwrite = true
	file = 'report.html'
}

timeline {
	enabled = true
	overwrite = true
	file = 'timeline.html'
}

trace {
	enabled = true
	overwrite = true
	file = 'trace.txt'
}

log {
	enabled = true
	file = 'nextflow.log'
}

params {
	in_string = 'Hello World'
}